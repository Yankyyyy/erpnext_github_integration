{
  "doctype": "DocType",
  "name": "Repository Pull Request",
  "module": "ERPNext",
  "custom": true,
  "is_submittable": false,
  "fields": [
    {
      "fieldname": "repository",
      "label": "Repository",
      "fieldtype": "Link",
      "options": "Repository",
      "reqd": 1
    },
    {
      "fieldname": "pr_number",
      "label": "PR Number",
      "fieldtype": "Int",
      "reqd": 1
    },
    {
      "fieldname": "title",
      "label": "Title",
      "fieldtype": "Data"
    },
    {
      "fieldname": "body",
      "label": "Body",
      "fieldtype": "Text"
    },
    {
      "fieldname": "state",
      "label": "State",
      "fieldtype": "Select",
      "options": "open\nclosed\nmerged"
    },
    {
      "fieldname": "head_branch",
      "label": "Head Branch",
      "fieldtype": "Data"
    },
    {
      "fieldname": "base_branch",
      "label": "Base Branch",
      "fieldtype": "Data"
    },
    {
      "fieldname": "author",
      "label": "Author",
      "fieldtype": "Data"
    },
    {
      "fieldname": "reviewers_table",
      "label": "Reviewers",
      "fieldtype": "Table",
      "options": "Repository PR Reviewer"
    },
    {
      "fieldname": "mergeable_state",
      "label": "Mergeable State",
      "fieldtype": "Data"
    },
    {
      "fieldname": "github_id",
      "label": "GitHub ID",
      "fieldtype": "Data"
    },
    {
      "fieldname": "url",
      "label": "URL",
      "fieldtype": "Data"
    },
    {
      "fieldname": "created_at",
      "label": "Created At",
      "fieldtype": "Datetime"
    },
    {
      "fieldname": "updated_at",
      "label": "Updated At",
      "fieldtype": "Datetime"
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "GitHub Admin",
      "read": 1,
      "write": 1
    }
  ]
}